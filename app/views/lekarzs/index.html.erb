 <P><strong>Lista Doktorów</strong></P>
  	<thead>
    <tr>
      <th>Imie</th>
      <th>Nazwisko</th>
      <th>Specjalizacja</th>
      <th>Nrtelefonu</th>
      <th>Email</th>
   
      <th colspan="3">  Edycja</th>
    </tr>
  </thead>
<br>
  <tbody>
    <% @lekarzs.each do |lekarz| %>
      <tr>
        <td><%= lekarz.imie %></td>
        <td><%= lekarz.nazwisko %></td>
        <td><%= lekarz.specjalizacja %></td>
        <td><%= lekarz.nrTelefonu %></td>
        <td><%= lekarz.email %></td>
    
        <td><%= link_to 'Pokaż', lekarz %></td>
        <td><%= link_to 'Edytuj', edit_lekarz_path(lekarz) %></td>
        <td><%= link_to 'Usuń', lekarz, method: :delete, data: { confirm: 'Czy napewno chcesz usunąć?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</p>
<br>
<p>
<%= link_to 'Dodaj lekarza', new_lekarz_path %>